<Window x:Class="ChatApiFront.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatApiFront"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="328,165,0,0" Text="Username" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="Username"/>
        <TextBox HorizontalAlignment="Left" Text="Password" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="328,199,0,0" TextChanged="Password"/>
        <Button Content="LOGIN" HorizontalAlignment="Left" Margin="360,252,0,0" VerticalAlignment="Top" Click="Login"/>

    </Grid>
</Window>
